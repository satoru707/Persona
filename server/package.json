{
  "name": "ai-timeboard-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "prisma generate && prisma migrate deploy && ts-node-dev --respawn --transpile-only src/index.ts | pino-pretty",
    "build": "tsc",
    "start": "prisma generate && prisma migrate deploy && npm run build &&node dist/index.js",
    "postinstall": "prisma generate",
    "prisma": "prisma",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.2",
    "express": "^4.18.2",
    "google-auth-library": "^9.6.3",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.8.0",
    "web-push": "^3.6.7"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.19",
    "@types/web-push": "^3.6.4",
    "pino-pretty": "^9.4.1",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
